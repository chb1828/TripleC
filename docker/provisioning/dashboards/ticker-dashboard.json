          "query": "import \"date\"\n\nstart = v.timeRangeStart\nstop = v.timeRangeStop\n\nlatest = from(bucket: \"triplec-bucket\")\n  |> range(start: start, stop: stop)\n  |> filter(fn: (r) => r._measurement == \"ticker\" and r._field == \"trade_price\" and r.code == \"KRW-BTC\")\n  |> last()\n\npast = from(bucket: \"triplec-bucket\")\n  |> range(start: date.sub(from: start, d: 24h), stop: start)\n  |> filter(fn: (r) => r._measurement == \"ticker\" and r._field == \"trade_price\" and r.code == \"KRW-BTC\")\n  |> last()\n\njoin(tables: {new: latest, old: past}, on: [\"code\"])\n  |> map(fn: (r) => ({\n    _time: r._time_new,\n    _value: ((r._value_new - r._value_old) / r._value_old) * 100.0,\n    code: r.code\n  }))",
